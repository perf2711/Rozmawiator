<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Rozmawiator.PartialViews"
             xmlns:Controls="clr-namespace:Rozmawiator.Controls" x:Class="Rozmawiator.PartialViews.SearchUsersControl"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="160">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition />
        </Grid.RowDefinitions>
        <Controls:ExtendedTextBox x:Name="SearchBox" HorizontalAlignment="Stretch" VerticalAlignment="Top" Height="23" KeyDown="SearchBox_KeyDown" Width="Auto" Description="Szukaj"/>
        <ScrollViewer Grid.Row="1" VerticalScrollBarVisibility="Hidden">
            <StackPanel x:Name="ResultPanel"></StackPanel>
        </ScrollViewer>
    </Grid>
</UserControl>
